config_version=5

[application]

config/name="FirebaseTutorial"
run/main_scene="res://Authentication.tscn"
config/features=PackedStringArray("4.4", "Mobile")
config/icon="res://icon.svg"

[autoload]

Firebase="*res://addons/godot-firebase/firebase/firebase.tscn"

[dotnet]

project/assembly_name="FirebaseTutorial"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-firebase/plugin.cfg")

[rendering]

renderer/rendering_method="mobile"
